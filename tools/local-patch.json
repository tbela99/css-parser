{
  "atrules": {
    "@stylistic": {
      "syntax": " @stylistic { <feature-value-declaration-list> } "
    },
    "@historical-forms": {
      "syntax": " @historical-forms { <feature-value-declaration-list> } "
    },
    "@styleset": {
      "syntax": " @styleset { <feature-value-declaration-list> } "
    },
    "@character-variant": {
      "syntax": " @character-variant { <feature-value-declaration-list> } "
    },
    "@swash": {
      "syntax": " @swash { <feature-value-declaration-list> } "
    },
    "@ornaments": {
      "syntax": " @ornaments { <feature-value-declaration-list> } "
    },
    "@annotation": {
      "syntax": " @annotation { <feature-value-declaration-list> } "
    }
  },
  "properties": {
    "-ms-overflow-style": {
      "syntax": "auto | none | scrollbar | -ms-autohiding-scrollbar"
    },
    "-moz-user-select": {
      "comment": "extend with auto",
      "syntax": "| auto"
    },
    "text-align": {
      "comment": "extend with -non-standard-text-align",
      "syntax": "| <-non-standard-text-align>"
    }
  },
  "types": {
    "track-breadth": {
      "comment": "extend track-breadth syntax with -non-standard-size",
      "syntax": "| <-non-standard-size>"
    },
    "-non-standard-display": {
      "comment": "extend display syntax with -ms-flexbox",
      "syntax": "| -ms-flexbox"
    },
    "-non-standard-text-align": {
      "comment": "extend text-align",
      "syntax": "| -moz-center | -webkit-center | -webkit-match-parent"
    }
  }
}